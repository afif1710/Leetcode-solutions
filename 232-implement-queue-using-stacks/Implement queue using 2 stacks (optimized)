class MyQueue {
public:
    stack<int> st1;
    stack<int> st2;
    int peekElement;
    MyQueue() {
        
    }
    
    void push(int x) {
        if(st1.empty()){
            peekElement = x;            //Whenever st1 is empty, we store the first pushed value as peekElement to use in the peek()
        }
        st1.push(x);            //Push the rest of the inputs as usual
        
    }
    
    int pop() {
        int tmp;
        if(!st2.empty()){        //Check if st2 has elements to pop from, to make it Amoritzed O(1), occassionally O(n)
            tmp = st2.top();
            st2.pop();
            
        }else{
            while(!st1.empty()){            //if st2 is empty then we need O(n) to push all values from st1 to st2
                st2.push(st1.top());
                st1.pop();
            }
            tmp = st2.top();                //Always return the top value from st2 for pop()
            st2.pop();
        }
        return tmp;
       
        
    }
    
    int peek() {
        if (st2.empty()) {
            return peekElement;
        }
        return st2.top();            // Return the top value from st2 for peek() if st2 is not empty, else return first value store in peekEl..
    }
    
    bool empty() {
        return (st1.empty() && st2.empty());
    }


};

/**
 * Your MyQueue object will be instantiated and called as such:
 * MyQueue* obj = new MyQueue();
 * obj->push(x);
 * int param_2 = obj->pop();
 * int param_3 = obj->peek();
 * bool param_4 = obj->empty();
 */
